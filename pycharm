#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2021-08-12T15:24:49.461462

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/tatsuroro/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-P/ch-0/212.4746.96/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"